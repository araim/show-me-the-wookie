<mui:ModernWindow x:Class="DependencyVisualizerGraphX.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:graphxctrl="clr-namespace:GraphX.Controls;assembly=GraphX.WPF.Controls"
        xmlns:gx="clr-namespace:GraphX;assembly=GraphX.WPF.Controls"
        xmlns:local="clr-namespace:DependencyVisualizerGraphX"  
        TextOptions.TextFormattingMode ="Ideal"
        UseLayoutRounding="False"
        ContentSource="/Pages/Config.xaml"
        Title="Show me the Wookie" IsTitleVisible="True"
        Height="450" Width="925">  
  <mui:ModernWindow.MenuLinkGroups>
    <mui:LinkGroup DisplayName="Start">
      <mui:LinkGroup.Links>
        <mui:Link DisplayName="Configuration" Source="/Pages/Config.xaml" />
      </mui:LinkGroup.Links>
    </mui:LinkGroup>
    <mui:LinkGroup DisplayName="Graph">
      <mui:LinkGroup.Links>
        <mui:Link DisplayName="Graph Presenter" Source="/Pages/GraphPresenter.xaml" />        
      </mui:LinkGroup.Links>
    </mui:LinkGroup>
  </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>


